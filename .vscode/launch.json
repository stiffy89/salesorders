{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds serve",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "serve",
        "--with-mocks",
        "--in-memory?"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "command" : "cds watch --profile hybrid",
      "name" : "cds watch hybrid",
      "request" : "launch",
      "type" : "node-terminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}